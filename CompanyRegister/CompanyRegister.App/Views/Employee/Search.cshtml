@model CompanyRegister.Models.Entity.Employee
@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>
<table class="table table-striped table-condensed table-bordered">
    <tr>
        <th>Name</th>
        <th>Info</th>
    </tr>
    <tr>
        <td>@Model.FirstName @Model.SecondName @Model.LastName</td>
        <td>@Html.ActionLink("Details", "Details", "Employee", new { id = Model.Id }, new { @id = "details" })</td>
    </tr>

</table>